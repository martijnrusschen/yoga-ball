{
  "name": "basecamp.com: replace Victoria with a better image.",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "name": "Basecamp",
    "icons": ["icon.png"]
  },
  "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["replaceChick.js"]
    }],
  "manifest_version": 2
}
